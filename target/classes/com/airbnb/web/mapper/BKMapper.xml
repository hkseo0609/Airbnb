<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.airbnb.web.mapper.BKMapper">
	<insert id="insert">
	</insert>
	
	<select id="selectList">
	</select>
   
	<select id="selectOne" resultType="com.airbnb.web.domain.Member"
		parameterType="com.airbnb.web.command.Command">
		SELECT *
		FROM Member 
		WHERE 
			member_id LIKE #{action} AND 
			member_password LIKE #{column}
	</select>
	
	<select id="count">  
	</select>

	<update id="update">
	</update>
	
	<delete id="delete">
	</delete>
	
</mapper>